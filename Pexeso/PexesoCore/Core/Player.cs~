using System;
namespace Pexeso.PexesoCore
{
    public class Player
    {
        public String Name { get; set; }
        public int Print { get; set; }
        public int Size { get; set; }

        public Player(string name, int print, int size)
        {
            Name = name;
            Print = print;
            Size = size;
        }

    }
}
